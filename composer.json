{
    "name": "kraenkvisuell/nova-settings",
    "description": "A Laravel Nova tool for editing custom settings using native Nova fields.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0.2|^8.1",
        "laravel/nova": "^4.0",
        "manogi/nova-translatable": "^2.0",
        "spatie/laravel-translatable": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "KraenkVisuell\\NovaSettings\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "KraenkVisuell\\NovaSettings\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KraenkVisuell\\NovaSettings\\NovaSettingsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
